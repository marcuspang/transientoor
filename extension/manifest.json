{
    "name": "Transientoor Extension",
    "description": "This extension allows for approvals and swaps to happen in one transaction",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://app.uniswap.org/*"
            ],
            "js": [
                "dist/background.js"
            ]
        }
    ]
}
